import org.apache.jmeter.services.FileServer;
import java.io.FileWriter;
import java.io.PrintWriter;
import java.util.Arrays;
import java.io.Writer;



var CatID= vars.get(&quot;CategoryID&quot;);
var Name= vars.get(&quot;Name&quot;);
var Path= vars.get(&quot;Path&quot;);
var PromotionID= vars.get(&quot;PromotionID_ALL&quot;);
var Price= vars.get(&quot;Price&quot;);


File file =new File (&quot;\\SampledataFromBeanShellDataSampler.csv&quot;);
FileWriter fstream = new FileWriter(file,true);

BufferedWriter out= new BufferedWriter(fstream);



out.write(CatID+&quot;,&quot;+Name+&quot;,&quot;+Path+&quot;,&quot;+PromotionID+&quot;,&quot;+Price);
out.write(&quot;\n&quot;);


out.close();
fstream.close();
</stringProp>
